include(CMakeFindDependencyMacro)
list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
include(AddYggInterface)
set(YGG_INSTALL_DEPS "@YGG_INSTALL_DEPS@")
set(YGG_INSTALL_CONFIG "@YGG_INSTALL_CONFIG@")
set(YGG_TARGET_BASE "@YGG_TARGET_BASE@")
if(NOT YGG_TARGET_BASE)
  set(YGG_TARGET_BASE ygg)
endif()
foreach(dep IN LISTS YGG_INSTALL_DEPS)
  if (dep STREQUAL "Python3" OR dep STREQUAL "Python" OR
      dep STREQUAL "ZeroMQ" OR dep STREQUAL "Julia" OR
      dep STREQUAL "rabbitmq-c")
    find_yggdrasil_dependency(${dep} FOR_PACKAGE_CONFIG)
  else()
    find_dependency(${dep})
  endif()
endforeach()
parse_yggdrasil_cmake_config()
include(${CMAKE_CURRENT_LIST_DIR}/YggInterfaceTargets.cmake)
if (TARGET ${YGG_TARGET_BASE})
  add_library(YggInterface::C ALIAS ${YGG_TARGET_BASE})
endif()
if (TARGET ${YGG_TARGET_BASE}++)
  add_library(YggInterface::CXX ALIAS ${YGG_TARGET_BASE}++)
endif()
if (TARGET f${YGG_TARGET_BASE})
  add_library(YggInterface::Fortran ALIAS f${YGG_TARGET_BASE})
endif()
set(CMAKE_MODULE_PATH ${CMAKE_MODULE_PATH} CACHE INTERNAL "CMAKE_MODULE_PATH")
